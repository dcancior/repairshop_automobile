<odoo>
    <record id="view_car_model_tree" model="ir.ui.view">
        <field name="name">car.model.tree</field>
        <field name="model">car.model</field>
        <field name="arch" type="xml">
            <tree string="Car Models">
                <field name="name" required="1"/>
                <field name="brand"/>
            </tree>
        </field>
    </record>
</odoo>